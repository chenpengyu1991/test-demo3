<%@page pageEncoding="UTF-8"%>
<script src="${pageContext.request.contextPath}/js/views/vaccine/hospital/add.js" type="text/javascript"></script>
<!-- 医院用户添加接种记录页面 -->
<div id="vaccineAdd" >
	<div class="vacopr" style="text-align: center;">
		<form method="post" id="vaccine_Type_Choose">
			<b>请选择接种疫苗的类型</b>
			<p>
			<select id="vaccineTypeC" name="vaccineTypeC" style="margin-top:10px; width: 150px;">
				<option value="rabies">狂犬疫苗</option>
				<option value="hepatitis">乙肝疫苗</option>
				<option value="influenza">流感疫苗</option>
				<option value="pneumonia">23价肺炎疫苗</option>
			</select>
			</p>
		</form>
	</div>
	
	<div class="vacbtn">
		<button class="layui-btn layui-btn-sm" id="vaccineConfirm"><i class="layui-icon"></i>确定</button>
	</div>
</div>
 
