.icon-username,.icon-password, .icon-checkcode, .icon-idcard, .icon-phone, .icon-email{
	display: inline-block;
    vertical-align: middle;
	width: 35px;
	height: 36px;
}
.icon-username {
	background: url(images/login_user_icon.png) no-repeat scroll 0 0;
}
.icon-password {
	background:url(images/login_password_icon.png) no-repeat scroll 0 0;
}
.icon-idcard {
	background: url(images/id_card.png) no-repeat scroll 0 0;
}
.icon-phone {
	background: url(images/cellphone.png) no-repeat scroll 0 0;
}
.icon-checkcode {
	background: url(images/login_password_icon.png) no-repeat scroll 0 0;
}
