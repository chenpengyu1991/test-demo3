.register-text {
	font-size:30px; 
	font-family:'黑体'; 
	padding-top:30px; 
	color:#000000;
}
.registerArea {
	padding-top:100px;
	margin-left:0px;
	color: #777777;
}
.registerArea ul li{
	width:677px;
	position:relative;
	margin-bottom:10px;
}
.registerArea ul li a{
	color: #696767;
}

.registerArea ul li span, input {
	height: 20px;
}
.registerArea ul li span {
	margin-top:5px;
	text-align:left;
	font-size:14px;
	position:absolute;
}
.register-input {
	background: #ebebeb none repeat scroll 0 0;
    border: 1px solid #ebebeb;
    border-radius: 5px;
    padding-bottom: 4px;
    padding-left: 10px;
    padding-top: 4px;
    margin-left: 10px;
    width: 190px;
}
.register-input:focus {
	border-color: #66afe9;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(102, 175, 233, 0.6);
    outline: 0 none;
}
.regCheckcode-input {
	background: #ebebeb none repeat scroll 0 0;
    border: 1px solid #ebebeb;
    border-radius: 5px;
    margin-left: 10px;
    padding-bottom: 4px;
    padding-left: 15px;
    padding-top: 4px;
    width: 100px;
}
.regCheckcode-input:focus {
   border-color: #66afe9;
   box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(102, 175, 233, 0.6);
   outline: 0 none;
}
.stepBtn {
    background: #0076bf none repeat scroll 0 0;
    color: #ebebeb;
    border-radius: 5px;
    display: inline-block;
    font-size: 15px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    width: 120px;
}
.stepBtn:hover {
    background: #157dbd none repeat scroll 0 0;
    text-decoration: none;
    cursor: pointer;
}
