	background: url(images/email.png) no-repeat scroll 0 0;
}
.login-input {
	background: #ebebeb none repeat scroll 0 0;
    border: 1px solid #ebebeb;
    border-radius: 5px;
    margin-left: 10px;
    padding-bottom: 8px;
    padding-left: 10px;
    padding-top: 8px;
    width: 190px;
}

.login-input:focus {
	border-color: #66afe9;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(102, 175, 233, 0.6);
    outline: 0 none;
}
.select_showbox {
    background: #ebebeb url("../../../images/btn/search_ico.png") no-repeat scroll 53px center;
    width: 65px;
    font-size:13px;
    border: 1px solid #ebebeb;
    border-radius: 5px;
    margin-left: 10px;
    padding-bottom: 7px;
    padding-top: 9px;
    display:-moz-inline-box;
	display:inline-block;
	position: relative;
	cursor: pointer;
}
.select_showbox .select_option {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: #fff none repeat scroll 0 0;
    border-color: -moz-use-text-color #ebebeb #ebebeb;
    border-image: none;
    border-style: none solid solid;
    border-width: medium 1px 1px;
    display: none;
    left: -1px;
    position: absolute;
    top: 35px;
    z-index: 99;
}
.select_showbox .select_option li {
    cursor: pointer;
    width: 65px;
    padding-bottom: 8px;
    padding-top: 8px;
    margin:0px;
    height:20px;
}
.select_showbox .select_option li.selected {
    background-color: #f3f3f3;
    color: #999;
}
.select_showbox .select_option li:hover {
    background: #bebebe none repeat scroll 0 0;
    color: #fff;
}
.checkcode-input {
	background: #ebebeb none repeat scroll 0 0;
    border: 1px solid #ebebeb;
    border-radius: 5px;
    margin-left: 10px;
    padding-bottom: 8px;
    padding-left: 10px;
    padding-top: 8px;
    width: 100px;
}

.checkcode-input:focus {
   border-color: #66afe9;
   box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(102, 175, 233, 0.6);
   outline: 0 none;
}
.login-btn {
    background: #0076bf none repeat scroll 0 0;
    color: #ebebeb;
    border-radius: 5px;
    display: inline-block;
    font-size: 18px;
    height: 38px;
    line-height: 38px;
    text-align: center;
    width: 100px;
}
.login-btn:hover {
    background: #157dbd none repeat scroll 0 0;
    text-decoration: none;
    cursor: pointer;
}
