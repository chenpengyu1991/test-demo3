/* 菜单*/
#menubox {
    position:absolute;
    /*width:80px;*/
    /*position:absolute;display:none; 这两个是必须的哦*/
    margin:0;
    background-color:white;
    border:gray solid 1px;
    filter:alpha(opacity=90);
    opacity: 0.90;
    top:30px;
    right:8px;
    display:none;
    padding:0px 1px 1px 1px;
} 
#menubox ul li {
    height: 22px;
    /*width: 80px;*/
    vertical-align:bottom; /*不加会导致不同浏览器的行间距不一样*/
    list-style: none;
    margin-top:1px;
    background-color:#e5e5e5;
}

#menubox ul li a {
    color: #4b4b4b;
    text-decoration: none;
    line-height: 22px;
    display: block;
    padding: 0px 5px;
    text-align:left;
}

#menubox ul li a:hover {
    background-color:#084673;
    color:#ffffff;
    text-decoration:none;
}

#menubox li a .selected {
    background-color:#084673;
    color:#ffffff;
}