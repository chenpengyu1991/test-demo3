.medicineSearch {
    border: 2px solid #0090DB;
    height: 35px;
    margin: 40px auto 0;
    width: 525px;
}
.medicineSearch .select_box {
    color: #999999;
    float: left;
    font-size: 12px;
    font-family: "微软雅黑";
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    line-height: 35px;
    position: relative;
    width: 90px;
}
.medicineSearch .select_showbox {
    background: rgba(0, 0, 0, 0) url("../../../images/btn/search_ico.png") no-repeat scroll 80px center;
    height: 35px;
    width: 90px;
}
.medicineSearch .select_option {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: #fff none repeat scroll 0 0;
    border-color: -moz-use-text-color #0090DB #0090DB;
    border-image: none;
    border-style: none solid solid;
    border-width: medium 2px 2px;
    display: none;
    left: -2px;
    position: absolute;
    top: 35px;
    z-index: 99;
}
.medicineSearch .select_option li {
    cursor: pointer;
    width: 90px;
}
.medicineSearch .select_option li.selected {
    background-color: #f3f3f3;
    color: #999;
}
.medicineSearch .select_option li:hover {
    background: #bebebe none repeat scroll 0 0;
    color: #fff;
}
.medicineSearch input.inp_srh, .medicineSearch input.btn_srh {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    border: medium none;
    float: left;
    height: 35px;
    line-height: 35px;
}
.medicineSearch input.inp_srh {
    outline: medium none;
    width: 365px;
    margin-left:10px;
}
.medicineSearch input.btn_srh {
    background: #0090DB none repeat scroll 0 0;
    color: #fff;
    font-family: "微软雅黑";
    font-size: 15px;
    width: 58px;
    cursor: pointer;
}
