.interaction_contents {
	vertical-align: middle;
	text-align: left;
}
.interaction_contents .messageTitle {
	margin-top:8px;
}
.interaction_contents ul li.messageTitle{
	width: 860px;
	border-bottom:1px dashed #DEDEDE;
	background:#f5f7f8  url(../../../images/icon1.jpg) no-repeat;
    font-size: 12px;
    height: 25px;
    padding:8px 0px 0px 30px;
    text-align: left;
    vertical-align: middle;
}
.interaction_contents ul li a{
	float: left;
	height: 35px;
	color: #2F2F2F;
}
.interaction_contents ul li a:hover {
	float: left;
	height: 30px;
	color: #006699;
}
.interaction_contents ul li span {
	float: right;
	margin-right:10px;
	height: 25px;
	color: #2F2F2F;
}

.messageForm {
	width:730px;
	display: none;
	padding:5px;
}
.messageForm table {
    table-layout: fixed;
    border-collapse: collapse;
    border-spacing: 0;
}
.messageForm td {
    color:#666;
    border: 1px solid #ddd;
    padding: 3px 5px;
    font-size:13px;
}
.messageForm input {
    border: 1px solid #ccc;
    height: 22px;
    line-height: 22px;
    padding: 2px;
    width: 180px;
    font-size:13px;
    font-family: inherit;
    font-weight: inherit;
    float:left;
}
.messageForm select {
	font-size:13px;
    font-family: inherit;
    font-weight: inherit;
    float:left;
}
.messageForm textarea {
    font-size:13px;
    color:#000000;
    font-family: inherit;
    font-weight: inherit;
    float:left;
}
.messageForm font, .messageForm span {
	float:left;
    text-align:left;
    padding-top:5px;
}
.messageForm .lefttdbg {
    width: 150px;
}

.messageForm .submitInput {
    background:url("../../../images/wyzx.png") no-repeat scroll 0 0;
    border: 0 none;
    cursor: pointer;
    height: 28px;
    padding: 0;
    width: 84px;
}
.messageForm .clearInput {
    background:url("../../../images/qk.png") no-repeat scroll 0 0;
    border: 0 none;
    cursor: pointer;
    height: 28px;
    padding: 0;
    width: 84px;
}

