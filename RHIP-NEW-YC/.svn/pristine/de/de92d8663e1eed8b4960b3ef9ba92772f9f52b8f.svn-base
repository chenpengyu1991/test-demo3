#phyExamForm .table-01{width:100%;clear:both;vertical-align:middle;}
#phyExamForm .table-01 table{width:100%; border-left:#C8BArrayAE solid 1px;border-top:#C8BArrayAE solid 1px;border-collapse:collapse;}
#phyExamForm .table-01 td{text-align:left;height:24px;line-height:24px;border:1px solid #ccc;padding-left:2px;}
.table-01 th{border:1px solid #CCC;background:#EFF7FF;font-weight:normal;}
.table-01 td.td_gray{background:#EEE;}
.table-01 td.h_top{background:#66B9DB; font-weight:bold;text-align:center;color:#FFF;width: 12%;}

.table-td01{display:table;text-align:left;margin:0;padding:0;margin-bottom:3px;clear:both;}
.table-td01 li{float:left;margin-right:20px;height:18px;line-height:18px;margin-bottom:2px;}
.table-td01 li.mc{width:30px;margin-right:2px;}
.table-td01 li.name{width:100px;border-bottom:1px solid #BBB;}
.table-td01 li.time{width:100px;margin-right:7px;}
.table-td01 li.time input{height:15px;}
.table-td01 li.date{width:20px;}

.sttb{background-color: gray; display: none; float:left;margin-right:5px; }
.sttb tr{line-height: 3px; width: 30px;}
.sttb td{line-height: 30px; width: 30px;}

.text_table{width:80px;table-layout:fixed;border-collapse:collapse;word-wrap:break-word;word-break:break-all;}
.text_table td{width:50%;}
.text_table td.lefttop{border-right:1px solid #ccc;padding-bottom:2px;padding-right:2px;}
.text_table td.righttop{border-bottom:1px solid #ccc;padding-bottom:2px;padding-left:2px;}
.text_table td.leftbottom{border-top:1px solid #ccc;padding-top:2px;padding-right:2px;}
.text_table td.rightbottom{border-left:1px solid #ccc;padding-top:2px;padding-left:2px;}
.text_table td input{width:30px;height:30px;}
.errbox{color: #FF0000;}
