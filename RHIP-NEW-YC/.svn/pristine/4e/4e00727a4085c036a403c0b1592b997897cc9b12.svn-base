.whch-left-nav{
    position: absolute;
    left: 13;
    z-index: 2;
    padding-top: 10px;
    background-color: #E3E7EC;
    width: 200px;
    max-width: 200px;
    overflow: auto;
    overflow-x:hidden;
    border-right: 1px solid #e5e5e5;
	font-size: 12px;
    /*width: 0px;*/
}

.whch-left-nav a {
	color: #333;
}
.whch-left-nav #nav li{
    border-bottom: 1px solid #e5e5e5;
}
.whch-left-nav #nav li:hover > a{
    /*color: blue;*/
}
.whch-left-nav #nav .current{
    background-color: rgba(0, 0, 0, 0.3);
}
.whch-left-nav #nav li a{
    font-size: 14px;
    padding: 10px 15px 10px 20px;
    display: block;
    cursor: pointer;
}
.whch-left-nav #nav li a cite{
    font-size: 14px;
}

.whch-left-nav #nav li .sub-menu{
    display: none;
}
.whch-left-nav #nav li .opened{
    display: block;
}
.whch-left-nav #nav li .opened:hover{
    /*background: #fff url() 0 0 no-repeat;*/
}
.whch-left-nav #nav li .opened .current{
    
}
.whch-left-nav #nav li .sub-menu li:hover{
    /*color: blue;*/
     /*background: #fff url() 0 0 no-repeat;*/
}
.whch-left-nav #nav li .sub-menu li a{
    padding: 12px 15px 12px 30px;
    font-size: 14px;
    cursor: pointer;
}
.whch-left-nav #nav li .sub-menu li .sub-menu li a{
    padding-left: 45px;
}
.whch-left-nav #nav li .sub-menu li a:hover{
    color: #148cf1;
}
.whch-left-nav #nav li .sub-menu li a i{
    font-size: 12px;
}
.whch-left-nav #nav li a i{
    padding-right: 10px;
    line-height: 14px;
}
.whch-left-nav #nav li .nav_right{
    float: right;
    font-size: 16px;
}
