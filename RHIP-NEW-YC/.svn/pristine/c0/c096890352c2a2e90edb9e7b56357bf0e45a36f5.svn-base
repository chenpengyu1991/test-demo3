package com.founder.rhip.ehr.dto;

public class TargetDTO {

	/**
	 * codeType为镇编码，中心编码或者镇编码
	 * */
	public static final Integer NO_SELECT = 0;
	public static final Integer GB = 1;
	public static final Integer CENTER = 2;
	public static final Integer STATION = 3;

	private String code;
	private Integer type;
	private String[] targetCodes;
	
	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String[] getTargetCodes() {
		return targetCodes;
	}

	public void setTargetCodes(String[] targetCodes) {
		this.targetCodes = targetCodes;
	}

}
