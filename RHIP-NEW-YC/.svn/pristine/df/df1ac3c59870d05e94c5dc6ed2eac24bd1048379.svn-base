.titlebar {
	height: 28px;
	line-height: 28px;
	background-color: #eeeeee;
	padding-left: 8px;
	margin: 0 auto;
	color: #000000;
	font-weight: bold;
}

.titlebar .title {
	float: left;
	width: 50%;
	_width: 48%;
}

.titlebar .toobar {
	float: right;
	width: 40%;
	font-size: 12px;
	text-align: right;
	padding-right: 10px;
	cursor: pointer;
}

/*--- formated table ---*/
.formtable {
	margin-bottom: 5px;
	top: 0;
	margin-top: -1px;
	height: 21px;
	line-height: 21px;
	border: 1px solid #a9c3d0;
}

.formtable td {
	text-align: left;
	border: 1px solid #a9c3d0;
}

.formtable th {
	text-align: right;
	background: #f6fcff;
	border: 1px solid #a9c3d0;
}

.formtable th,.formtable td {
	vertical-align: middle;
	padding: 3px;
	font-weight: normal;
	border: 1px solid #a9c3d0;
}

.formtable td input[type="text"],.formtable td input[type="password"],.formtable td select
	{
	height: 22px;
	line-height: 22px;
	border: 1px solid #D7D7D7;
	/* 	width: 96%; */
}

.formtable td textarea {
	border: 1px solid #D7D7D7;
	width: 96%;
}

.formtable .SubTitle {
	color: #b30d0d;
}




.pagination {
	background: url(../../images/page_bg.gif) repeat-x;
	padding: 0px;
}

.pagination p.left {
	float: left;
	margin-left: 15px;
	height: 22px;
	line-height: 24px;
}

.pagination p.right {
	float: right;
	margin-right: 5px;
	height: 22px;
	line-height: 24px;
}

* html .pagination p.right {
	float: right;
	height: 20px;
	line-height: 24px;
	margin-top: 6px;
	margin-right: 5px;
}

* html .pagination font {
	font-size: 13px;
	vertical-align: top;
}

*+html .pagination font {
	vertical-align: middle;
	font-size: 12px;
}

.pagination p.right img {
	margin: 3px 2px 4px 2px;
}

* html .pagination p.right img {
	margin: -1px 2px 4px 2px;
}

*+html .pagination p.right img {
	margin: 4px 2px 4px 2px;
}

.stepbar {
	background: url(../../images/stepbg.gif) repeat-x;
	border-right: 1px solid #cfcfcf;
	border-left: 1px solid #cfcfcf;
	height: 26px;
	margin-bottom: 5px;
}

.stepbar ul li {
	float: left;
	padding: 0px 20px;
	height: 26px;
	line-height: 26px;
	list-style: none;
	background: url(../../images/step02.gif) no-repeat bottom right;
}

.stepbar ul li.stepleft {
	width: 10px;
	height: 26px;
	background: url(../../images/step03.gif) no-repeat;
	padding: 0;
	margin-left: -10px;
}

.stepbar ul li.steplight {
	color: #FFF;
	background: url(../../images/steplightbg.gif) no-repeat right top;
}
.borderDiv{
	border-bottom: 1px solid #A9C3D0;
	border-left: 1px solid #A9C3D0;
	border-right:  1px solid #A9C3D0;
}