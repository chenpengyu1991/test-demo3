.pagination-x {
    font-size: 12px;
    margin-top: 20px;
    width: 100%;
}

.pagination-x .pagers {
    float: right;
    margin-bottom: 10px;
}

.pagination-x .prev-on {
    background: url("../images/btn/button-back-light.png") no-repeat scroll 0 0;
}

.pagination-x .prev-off {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #fbfbfb !important;
    background-image: url("../images/btn/button-back-gray.png");
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-size: auto auto;
    border: 1px solid #e8e8e8 !important;
    color: #e0e0e0;
    opacity: 0.5;
}

.pagination-x .pointer-hand {
    cursor: pointer !important;
}

.pagination-x .prev-on, .pagination-x .prev-off {
    background-position: 10% 50%;
    background-size: 20px auto;
    padding-left: 26px;
}

.pagination-x .next-on {
    background: url("../images/btn/button-next-light.png") no-repeat scroll 0 0;
}

.pagination-x .next-off {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #fbfbfb !important;
    background-image: url("../images/btn/button-next-gray.png");
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-size: auto auto;
    border: 1px solid #e8e8e8 !important;
    color: #e0e0e0;
    opacity: 0.5;
}

.pagination-x .end-off {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #fbfbfb !important;
    background-origin: padding-box;
    background-size: auto auto;
    border: 1px solid #e8e8e8 !important;
    color: #e0e0e0;
    opacity: 0.5;
}

.pagination-x .next-on, .pagination-x .next-off {
    background-position: 87% 50%;
    background-size: 20px auto;
    padding-right: 26px;
}
.pagination-x .end-off {
	height: 20px;
    overflow: hidden;
}
.pagination-x .next-on, .pagination-x .next-off, .pagination-x .prev-on, .pagination-x .prev-off {
    height: 20px;
    overflow: hidden;
    width: 41px;
}
.pagination-x .ellipsis {
    background: transparent none repeat scroll 0 0 !important;
    border: 0 none !important;
    margin-right:8px;
}
.pagination-x ul {
    border-radius: 4px;
    display: inline-block;
    margin-bottom: 0;
    margin-left: 0;
}

.pagination-x ul > li {
    display: inline;
    float:left;
}
.pagination-x ul > li > a {
    background-color: #fff;
    border: 1px solid #eaeaea;
    float: left;
    line-height: 20px;
    padding: 4px 10px;
    text-decoration: none;
}
.pagination-x ul > li > a:hover, .pagination-x ul > li > a:focus, .pagination-x ul > .active > a, .pagination-x ul > .active > span {
    background-color: #f5f5f5;
}
.pagination-x ul > .active > a, .pagination-x ul > .active > span {
    color: #999;
    cursor: default;
}
.pagination-x ul > .disabled > span, .pagination-x ul > .disabled > a, .pagination-x ul > .disabled > a:hover, .pagination-x ul > .disabled > a:focus {
    background-color: transparent;
    color: #999;
    cursor: default;
}
.pagination-x ul > li:first-child > a {
    border-bottom-left-radius: 4px;
    border-left-width: 1px;
    border-top-left-radius: 4px;
}
.pagination-x ul > li:last-child > a {
    border-bottom-right-radius: 4px;
    border-top-right-radius: 4px;
    margin-left: 4px;
}
.pagination-x-list li a {
    cursor: default;
}
.pagination-x-list li a.currentpage {
    /* background: #eaeaea none repeat scroll 0 0; */
    color: #ff7900;
}



