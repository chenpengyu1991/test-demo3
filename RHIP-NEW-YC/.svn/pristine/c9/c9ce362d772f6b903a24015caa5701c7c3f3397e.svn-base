package com.founder.rhip.ehr.dto;

import javax.persistence.Column;
import java.math.BigDecimal;

//家庭档案报表统计用Dto
public class FamilyStatisticDto {

    private Integer familyNum = 0;

    private Integer recordFamilyNum = 0;

    private BigDecimal recordFamilyRate;

    private String orgCode;

    private String centerOrgCode;

    private String gbCode;

    public BigDecimal getRecordFamilyRate() {
        return recordFamilyRate;
    }

    public void setRecordFamilyRate(BigDecimal recordFamilyRate) {
        this.recordFamilyRate = recordFamilyRate;
    }

    public String getOrgCode() {
        return orgCode;
    }

    public void setOrgCode(String orgCode) {
        this.orgCode = orgCode;
    }

    public String getCenterOrgCode() {
        return centerOrgCode;
    }

    public void setCenterOrgCode(String centerOrgCode) {
        this.centerOrgCode = centerOrgCode;
    }

    public String getGbCode() {
        return gbCode;
    }

    public void setGbCode(String gbCode) {
        this.gbCode = gbCode;
    }

    public Integer getFamilyNum() {
        return familyNum;
    }

    public void setFamilyNum(Integer familyNum) {
        this.familyNum = familyNum;
    }

    public Integer getRecordFamilyNum() {
        return recordFamilyNum;
    }

    public void setRecordFamilyNum(Integer recordFamilyNum) {
        this.recordFamilyNum = recordFamilyNum;
    }
}
