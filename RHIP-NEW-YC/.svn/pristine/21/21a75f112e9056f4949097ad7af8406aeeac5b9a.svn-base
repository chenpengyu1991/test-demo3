<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.founder.elb.jtadb</groupId>
	<artifactId>elb</artifactId>
	<packaging>pom</packaging>
	<version>1.0.1</version>
	<name>ELB Parent</name>
	<url>http://www.founder.com</url>
	
		<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<copyright.year>2011</copyright.year>
        <project.fasf.groupId>com.founder.fasf.jtadb</project.fasf.groupId>
        <project.fasf.version>1.0.1</project.fasf.version>
		<project.groupId>com.founder.elb.jtadb</project.groupId>
		<project.version>1.0.1</project.version>
		  <springframework.version>4.0.5.RELEASE</springframework.version>
		<junit.version>4.10</junit.version>
		<displaytag.version>1.2</displaytag.version>
		<jstl.version>1.2</jstl.version>
		<servlet.version>2.5</servlet.version>
		<jsp.version>2.2</jsp.version>
			<slf4j.version>1.7.25</slf4j.version>
	</properties>
		<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.1</version>
					<configuration>
						<source>1.7</source>
						<target>1.7</target>
						<encoding>UTF-8</encoding>
					</configuration>
				</plugin>
				
			</plugins>
		</pluginManagement>
	</build>
	<modules>
		<module>elb-core</module>
		<module>elb-repository</module>
		<module>elb-service</module>
        <!--<module>elb-cas</module>-->
		<!--<module>elb-web</module>-->

	</modules>
	
	
</project>