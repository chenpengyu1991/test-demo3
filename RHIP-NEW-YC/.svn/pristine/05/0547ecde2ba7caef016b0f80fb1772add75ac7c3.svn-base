package com.founder.rhip.ehr.dto;

import java.util.List;

import com.founder.rhip.ehr.entity.clinic.DiseaseDiagnosisInfo;
import com.founder.rhip.ehr.entity.clinic.OuthospitalSummary;
import com.founder.rhip.ehr.entity.clinic.SurgeryInfo;

/**
 * 出院小结
 *
 * @author liuk
 */
public class OuthostpitalSummaryDTO {

    // 入院诊断
    private List<DiseaseDiagnosisInfo> inHosDiseaseDiagnosisInfos;

    // 出院诊断
    private List<DiseaseDiagnosisInfo> outHosDiseaseDiagnosisInfos;

    // 手术信息
    private List<SurgeryInfo> surgeryInfos;

    // 出院小结
    private OuthospitalSummary outhospitalSummary;

    public List<DiseaseDiagnosisInfo> getInHosDiseaseDiagnosisInfos() {
        return inHosDiseaseDiagnosisInfos;
    }

    public void setInHosDiseaseDiagnosisInfos(List<DiseaseDiagnosisInfo> inHosDiseaseDiagnosisInfos) {
        this.inHosDiseaseDiagnosisInfos = inHosDiseaseDiagnosisInfos;
    }

    public List<DiseaseDiagnosisInfo> getOutHosDiseaseDiagnosisInfos() {
        return outHosDiseaseDiagnosisInfos;
    }

    public void setOutHosDiseaseDiagnosisInfos(List<DiseaseDiagnosisInfo> outHosDiseaseDiagnosisInfos) {
        this.outHosDiseaseDiagnosisInfos = outHosDiseaseDiagnosisInfos;
    }

    public List<SurgeryInfo> getSurgeryInfos() {
        return surgeryInfos;
    }

    public void setSurgeryInfos(List<SurgeryInfo> surgeryInfos) {
        this.surgeryInfos = surgeryInfos;
    }

    public OuthospitalSummary getOuthospitalSummary() {
        return outhospitalSummary;
    }

    public void setOuthospitalSummary(OuthospitalSummary outhospitalSummary) {
        this.outhospitalSummary = outhospitalSummary;
    }


}
