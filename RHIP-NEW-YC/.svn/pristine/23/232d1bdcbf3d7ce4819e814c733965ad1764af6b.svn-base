/* section_ehr */
.section_ehr , .section_ehr_list {margin:1px 1px 2px 1px;border:1px solid #dbdbdb;;position:relative;background-color:#fff;padding-bottom:1px;}
.section_ehr .header , .section_ehr_list .header{padding-right:4px;text-align:right;color:#4a4a4a;background:url(../images/list_title_bg1.gif) repeat-x 0px 0px;height:28px;line-height:28px;_line-height:26px;}
.section_ehr .header .title , .section_ehr_list .header .title {float:left;text-align:center;margin:3px 0px 0px 3px;width:100px;font-weight:bold;clear:both;/*background:url(../images/tab.jpg) repeat-x;border:1px solid #BED5F3;border-bottom:0px;border-radius:5px 5px 0px 0px;-moz-border-radius: 5px 5px 0px 0px;-webkit-border-radius: 5px 5px 0px 0px;*/}
.section_ehr .header .title , .section_ehr_list .header .title{background:url(../images/tab_d1.gif) no-repeat left top;color:#4a4a4a;line-height:25px;}
.section_ehr_list .header .title {margin-top:2px;line-height:26px;}

.section_ehr .header a , .section_ehr_list .header a {float:right;margin-right:10px;color:#333;}
.section_ehr .header a{border-right:1px solid #72ABE5;border-left:1px solid #C9E4FF;margin-right:0px;padding:0px 10px 0px 35px;}

.section_ehr .content , .section_ehr_list .content {margin:1px 2px 2px 2px;padding:0px;display:block;}

.section_ehr .opucker, .section_ehr .pucker , .section_ehr_list .opucker , .section_ehr_list .pucker {height:17px;width:17px;display:block;margin:4px 0px 0px 4px;}
.section_ehr .opucker , .section_ehr_list .opucker {background: url(../images/icon/icon_g.gif) no-repeat;}
.section_ehr .opucker:hover , .section_ehr_list .opucker:hover {background: url(../images/icon/icon_g.gif) no-repeat;}
.section_ehr .pucker , .section_ehr_list .pucker {background: url(../images/icon/icon_s.gif) no-repeat;}

/*----------info-detailed---------*/
.info-detailed fieldset{margin:10px;border:2px solid #88C3FF;text-align: left;*text-align: center;}
.info-detailed .repeattable{width: 98%;margin-top: 4px;}
.info-detailed fieldset legend{ font-weight:bold;margin:5px;padding:5px 10px;color:#FFF;background:#498ACE;}
.form{margin:0 10px;padding:10px;}

.fieldset_table{width: 98%;margin-top: 4px;margin-bottom: 4px;}
.fieldset_table th , .fieldset_table td {padding:2px;height:24px;line-height:24px;border:1px solid #C7DAEC;background:#FFF;}
.fieldset_table th {text-align:right; font-weight:normal;width:100px;padding-right:5px;background:#DDEBFF;}
.fieldset_table td {text-align: left;}
.fieldset_table textarea{width:99%;}
.fieldset_table input {border:1px solid #DDD;background:#F6F6F6;color:#4a4a4a;}
.fieldset_table input.inputover {border:1px solid #7F9DB9;background:#FFF;}
.fieldset_table input.inputout {border:1px solid #DDD;background:#F6F6F6;}
.fieldset_table select {background:#F6F6F6;border:1px solid #DDD;}
.fieldset_table .btn {filter:progid:dximagetransform.microsoft.gradient(gradienttype=0, startcolorstr=#ffffff, endcolorstr=#cecfde);cursor:hand;color:black;line-height:20px;padding-left: 10px;padding-right: 10px;padding-top: 2px;padding-bottom: 2px;border:1px solid #7b9ebd;height:25px;}

.fieldset_table1 th , .fieldset_table1 td {padding:2px;height:36px;line-height:36px;background:#FFF;padding:2px 5px;border:1px solid #EEE;border-left:none;border-right:none;}
.fieldset_table1 th {text-align:right; font-weight:normal;width:200px;padding-right:5px;}
.fieldset_table1 textarea{width:100%;}
.fieldset_table1 th.bg-blue , .fieldset_table1 td.bg-blue {background:#DDEBFF;border:1px solid #FFF;border-left:none;border-right:none;} 
.fieldset_table1 th.border-top , .fieldset_table1 td.border-top {border-top:1px solid #C7DAEC;}
.fieldset_table1 th.border-bottom , .fieldset_table1 td.border-bottom {border-bottom:1px solid #C7DAEC;}

/*-------spacetable--------*/
.spacetable {margin:5px 0px 0px;background-color:#DDEBFF;border:1px solid #BFCFDA;}
.spacetable td , .spacetable th {font-weight:normal;height:32px;line-height:32px;}
.spacetable td {text-align:left;padding-left:3px;padding-right:5px;*padding-right:7px;padding-right:7px\0;}
.spacetable th {text-align: right;padding-right:5px;}
.spacetable input , .spacetable select {background-color:#F0F6FF;height:22px;vertical-align:middle;line-height:22px;border-bottom:1px solid #BFD4E8;border-top:1px solid #AEBFD1;border-right:1px solid #BFD4E8;border-left:1px solid #AEBFD1;}
.spacetable .btn {filter:progid:dximagetransform.microsoft.gradient(gradienttype=0, startcolorstr=#ffffff, endcolorstr=#cecfde);cursor:hand;color:black;line-height:20px;padding-left: 10px;padding-right: 10px;padding-top: 2px;padding-bottom: 2px;border:1px solid #7b9ebd;height:25px;}


.section_ehr a.pucker , .section_ehr a.opucker , .section_ehr a.opucker:hover , .section_ehr a.pucker:hover {border:none;padding:0px;margin-top:5px;}
button{outline:0;zoom:1;background:#f2f2f2;cursor:hand;}
.btn {filter:progid:dximagetransform.microsoft.gradient(gradienttype=0, startcolorstr=#ffffff, endcolorstr=#cecfde);cursor:hand;color:black;line-height:20px;padding-left: 10px;padding-right: 10px;padding-top: 2px;padding-bottom: 2px;border:1px solid #7b9ebd;height:25px;background-color: #e6e6e6}
.btn35 , .btn36{width:100px;height:28px;line-height:26px;font-size:12px;color:#444;background:url("../images/button/bg35.jpg") no-repeat left top;padding-bottom:4px;border:none;}
.btn36 {background:url(../images/button/bg36.jpg) no-repeat left -28px;color:#fff;}

.addHref{font-weight: bold;margin-top: -10px;margin-right:12px;float: right;font-size: 12px;}
