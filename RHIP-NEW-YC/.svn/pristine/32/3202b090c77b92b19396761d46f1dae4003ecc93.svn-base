@charset "utf-8";
#communityInfoId {width:295px;float:right;border:1px solid #CCC;background:url(../../../../images/h6-bg.gif) repeat-x top #FFF;margin-bottom:6px;}
#communityInfoId b {height:31px;line-height:35px;background:url(../../../../images/search-ico.gif) no-repeat left;padding-left:45px;color:#0D81AF; display:block;}
#communityInfoId b.jiandang{background:url(../../../../images/ico_1.gif) no-repeat left;}
#communityInfoId table{margin:5px;line-height:30px;}
#communityInfoId table select{width:100px;}

#main-search #communityInfoId {clear:both;width:100%;}

/*----人口服务------*/
#communityInfoId b a.xiug{background:url(../../../../images/btn/pencil.png) no-repeat left 8px;margin:0;padding:0;padding-left:20px;float:right;margin-right:10px;font-weight:normal;}

.table-pop{width:99%;height:96%; overflow:auto;clear:both;vertical-align:middle;}
.table-pop{background:#FFF}
.table-pop table{width:100%; border-left:#C8BArrayAE solid 1px;border-top:#C8BArrayAE solid 1px;border-collapse:collapse;} 
.table-pop td{text-align:left;height:15px;line-height:15px;border:1px solid #ccc;padding:1px;}
.table-pop th{border:1px solid #CCC;background:#EFF7FF;font-weight:normal;}
.table-pop td.td_gray{background:#EEE;}
.table-pop td.h_top{background:#e6e6e6; font-weight:bold;text-align:center;color:#000;}