package com.founder.rhip.portal.common;

public class EmailInfo {

	private EmailAccount from = null; // 发件人地址
	private String to = ""; // 收件人地址
	private String subject = ""; // 邮件标题
	private String content = "";// 邮件内容

	public EmailInfo() {
	}

	public EmailInfo(EmailAccount from, String to, String subject,String content) {
		this.from = from;
		this.to = to;
		this.subject = subject;
		this.content=content;
	}

	public EmailAccount getFrom() {
		return from;
	}

	public void setFrom(EmailAccount from) {
		this.from = from;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public String getSubject() {
		return subject;
	}

	public void setSubject(String subject) {
		this.subject = subject;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}
}
