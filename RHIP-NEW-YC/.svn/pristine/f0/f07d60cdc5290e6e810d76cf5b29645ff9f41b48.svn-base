	height: 22px;
}

#Loginbar p.left {
	float: left;
	height: 28px;
	line-height: 28px;
	padding-left: 10px;
}

#Loginbar p.right {
	float: right;
	height: 28px;
	line-height: 28px;
	padding-right: 10px;
}

#Loginbar img {
	padding-right: 2px;
	vertical-align: middle;
}

#Loginbar p span img {
	padding-left: 5px;
	padding-right: 5px;
}

#Loginbar .right a:link,#Loginbar .right a:visited,#Loginbar a:hover,#Loginbar a:active
