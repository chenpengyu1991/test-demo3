/*左边菜单样式*/
#sidemenuwrapper {
	width: 200px;
	/*//border: 1px solid #CCC;*/
}

.sidebartop {
	background: #FFF url(../../images/menu.jpg) no-repeat 0px -219px;
	height: 43px;
}

.sidemenu {
	padding: 1px;
	margin: 0 auto;
	cursor: pointer;
}

.sidemenu * {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
}

.sidemenulist span {
	font-style: normal;
	font-weight: bold;
	width: 100%;
	display: block;
	cursor: pointer;
	color: #004376;
	text-decoration: none;
}

.sidemenulist a {
	color: #4A4A4A;
	text-decoration: none;
	font-size: 12px;
	display: block;
	font-weight: normal;
}

.sidemenulist .active {
	background: #498ACE !important;
}

.sidemenulist .active a {
	color: #FFFFFF !important;
	font-weight: bold;
}

.sidemenulist li.sidemenu01.collapsable {
	background: url(../../images/menu.jpg) no-repeat 0px -35px;
}

.sidemenulist li.sidemenu01.expandable {
	background: url(../../images/menu.jpg) no-repeat 0px 0px;
}

.sidemenulist span.sidemenu01 {
	text-indent: 28px;
	height: 35px;
	line-height: 35px;
	width: 100%;
}

.sidemenulist li ul li.sidemenu02.collapsable {
	background: url(../../images/menu.jpg) no-repeat 0px -101px;
}

.sidemenulist li ul li.sidemenu02.expandable {
	background: url(../../images/menu.jpg) no-repeat 0px -70px;
}

.sidemenulist li ul li span.sidemenu02 {
	text-indent: 41px;
	color: #4A4A4A;
	font-weight: normal;
	vertical-align: middle;
	height: 31px;
	line-height: 31px;
	width: 100%;
}

.sidemenulist li ul li.sidemenu022 {
	background: url(../../images/menu.jpg) no-repeat 0px -188px;
}

.sidemenulist li ul li span.sidemenu022 {
	text-indent: 18px;
	width: 100%;
	height: 31px;
	line-height: 31px;
}

.sidemenulist li ul li.sidemenu022.active {
	background: url(../../images/menu.jpg) no-repeat 0px -188px;
}

.sidemenulist li ul li.sidemenu022 a {
	color: #4A4A4A;
	width: 100%;
	text-indent: 41px;
}

.sidemenulist li ul li ul li.sidemenu03 {
	background: url(../../images/menu.jpg) no-repeat 0px -132px;
	text-indent: 55px;
	height: 28px;
	text-overflow: ellipsis;
    overflow: hidden;
}

.sidemenulist li ul li ul li.sidemenu03 a {
	color: #4A4A4A;
	height: 28px;
	line-height: 28px;
}

.sidemenulist li ul li ul li.sidemenu03 a:hover {
	background: url(../../images/menu.jpg) no-repeat 0px -160px;
	color: #0066B1 !important;
}

.shortCut ul li {
	list-style-type: none;
	display: inline;
	padding-right: 25px;
}

