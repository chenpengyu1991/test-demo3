/*开关按钮样式*/
.SwitchBar {
	background: #ffffff;
    background:url(../../images/bg_loginbarh.gif) repeat-y;
	vertical-align: middle;
	position: absolute;
	left: 200px;
	top: 60px;
	width: 8px;
	bottom: 33px;
	overflow: hidden;
}

.SwitchBar a {
	display: block;
	width: 5px;
	height: 51px;
	background: url(../../images/blue-icon.gif) center no-repeat;
	position: relative;
	top: 50%;
}

.SwitchBar a.open {
	display: block;
	width: 5px;
	height: 51px;
	background: url(../../images/blue-icon-2.gif) center no-repeat;
	position: relative;
