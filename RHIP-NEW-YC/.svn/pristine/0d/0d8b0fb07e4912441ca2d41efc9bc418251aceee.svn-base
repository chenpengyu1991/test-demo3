@charset "utf-8";
/* CSS Document */


/*--------标题-----------*/
#header i{background: url(../../images/vaccine/img_top.gif);background-repeat:no-repeat}
/* #header b{background-repeat} */

/*--------接种记录标题-----------*/
.vacsubtitle {
	corlor:#333333;
	font-family:Arial;
	text-align: center;
	font-size: 28px; 
	font-weight: bold; 
	font-style: normal; 
	text-decoration: none; 		
	border-collapse:collapse;
	height: 33px;
}

/*--------   -----------*/
.vacmainsearch{
	margin-top:5px;
	margin-left:5px;
	margin-bottom:5px;
	margin-right:5px;
	/* color:#0d81af; */
	font-size:16px;
	/* border-collapse:collapse; */
	}
.vactable{
	width:100%;
	height:100%;
	overflow:auto;
	clear:both;
	vertical-align:middle;
	border-left:#ccc 1px  solid;
	border-top:#ccc 1px  solid;
	border-bottom:#ccc 1px  solid;
	border-right:#ccc 1px  solid;
	float:right;
	background:url(../../images/h6-bg.gif) repeat-x 50%top #FFF;
	margin-bottom:6px;
	padding-top:8px;
	padding-bottom:8px;
	}
.vactable td{
	text-align:left;
	height:24px;
	line-height:24px;
	border:1px solid #ccc;
	padding:2px;
}
.vactable th{
	border:1px solid #CCC;
	background:#EFF7FF;
	font-weight:normal;
/*	padding-left:10px;*/
	text-align:center;
	line-heigth:30px;
	font-size:12px;
/* 	width: 100px; */
	}
/*--------小标题-----------*/
.vacmainsearchb{
	/*font-size:16px;*/
	color:#0d81af;
	line-heigth:35px;
	padding-left:45px;
	display:block;
	background:no-repeat left 50%;
	height:31px;
}
/*--------按钮-----------*/
.vacbtn{
	margin-top:10px;
	text-align:center;
}
/*--------操作返回-----------*/
.vacopr{
	margin-top:50px;
	text-line:center;
	color:#0d81af;
	font-size:16px;
	}
/*--------备注-----------*/
.vacnte{
	padding-top:5px;
	padding-bottom:5px;
	padding-left:3px;
	border-top:#ccc 1px  solid;
	border-right:#ccc 1px  solid;
	border-bottom:#ccc 1px  solid;
	border-left:#ccc 1px  solid;
	height:60px;
	width:99%; 
}
	/*--------必填项星号标志-----------*/
	.vaccstar{
	color:#ff0000;
	position:relative;
	 top:-5px;
	}
 
 
	 /*--------验证提示信息-----------*/
	.vacmsgError {
	cursor: pointer;
	cursor: hand;
	background: #FFE7E7 url('../../images/msg_error.png') center no-repeat;
	background-position: 15px 50%;
	text-align: left;
	padding: 15px 15px 15px 65px;
	border-top: 4px solid #E77D7B;
	border-bottom: 4px solid #E77D7B;
	border: 2px solid #E77D7B;
	overflow: auto;
	font-size: 12px;
	font-family: Verdana;
	margin-left:2px;
	margin-right:2px;
	width:99%;
}
	