body{
	margin:0 auto;
	padding:0px;
	font-family:"����";
	font-size:12px;
	color:#565656;
	font-weight:normal;
	line-height:18px;
	overflow-y: scroll
}

a img{border:none;}

a{
	color:#666666;
	text-decoration:none;
} 
a:hover {
	color:#3399CC;
	text-decoration:underline;
}




ul, li{padding:0; margin:0; list-style:none;}
ul {padding:0px;}

.cardcontentbg{
	float:left;
	min-width:720px;

	margin:10px 0px 10px 10px;
}

.cardcontent{
	float:left;
	width:185px;
	height:140px;
	border:#C0c0c0 solid 1px;
	border-radius:4px;
	margin-right:25px;
	margin-bottom:10px;
	}
.cardcontentright{
	float:left;
	width:180px;
	height:140px;
	border:#C0c0c0 solid 1px;
	border-radius:4px;
	margin-bottom:5px;
	}
.tabtitle{
	float:left;
	width:180px;
	height:32px;
	border-radius:4px;
	border-radius-topright:4px;
	border-radius-topleft:4px;
	background:url(../../images/tabbg.jpg) repeat-x;
	line-height:30px;
	font-size:12px;
	color:#0d81af;
	font-weight:bold;
	padding-left:5px;
	}
.tabcontent{
	float:left;
	width:180px;
	height:83px;
	padding-left:5px;
	padding-top:5px;
	}
.formcontentbg{
	float:left;
	margin:10px;	
}