.doverlay {
	position:absolute;
	left:0;
	top:0;
	background: #aaaaaa;
	opacity: .30;
	filter: Alpha(Opacity = 30);
}

.dialog{
	position:absolute;
	font-size: 12px;
	border:1px solid #596f7d;
    -moz-border-radius: 5px;
    -khtml-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    background-color: #fff;
}
.dialog .dialogtitle{
	 position:relative;
	 background:url(images/pop_hearder_bg.jpg) repeat-x;
	 height:32px;
	 line-height:32px;
	 font-size: 12px;
     color: #000000;
     border-top-left-radius: 6px;
     border-top-right-radius: 6px;
	 border-bottom:1px solid #fff;
}

.dialog .dialogtitle i{
	/* background:url(images/pop_hearder_ico.gif) no-repeat top left; */
	display:block;
	/* padding-left:35px; */
	font-weight:bold;
}

.dialog .dialogtitle s{
	position:absolute;
	top:8px;
	right:5px;
	background:url(images/cross.png) no-repeat;
	padding-left:20px;
	cursor: pointer;
}

.dialog .contentDiv{
	margin:0;
    padding-top: 8px;
	overflow-y: auto;
	overflow-x: hidden;
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
    background:url(images/conten_bg.gif) repeat-x;
/* 	text-align: center; */
/* 	padding:3px; */
/* 	border-bottom: 2px solid #88C3FF; */
/* 	border-left: 2px solid #88C3FF; */
/* 	border-right: 2px solid #88C3FF; */
    height: 91%;

}
.founderSelectDiv{
	background: #ffffff;
	border-top: 1px solid #D7D7D7;
	border-left: 1px solid #D7D7D7;
	border-right: 1px solid #D7D7D7;
	border-bottom: 1px solid #D7D7D7;
	padding: 1px;
}

.founderSelectDiv p{
	text-align:left;
	font-size: 12px;
	cursor: pointer;
	padding-top: 2px;
	text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
	margin: 0; 
}

.line1{
	background: #D7D7D7;
}

.pover{
	background: #69ACF5;
}

.founderSelectDiv .footer{
	padding-top:4px;
	text-align: center;
	font-size: 12px;
}
.leftButton{
	text-align: center;
	border: 1px solid;
	cursor: pointer;
}
#cancel{
    width: 60px;
    height: 23px;
}