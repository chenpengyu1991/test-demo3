@charset "utf-8";
/* CSS Document */
.section {
    width: 98%;
    margin: 10px 8px 0px 8px;
    padding: 0;
    min-width:720px;
   /*  border: 1px solid #D5D5D5; */
    position:relative;
}

.titlebar {
    height: 28px;
    line-height: 28px;
    background-color: #eeeeee;
    padding-left: 8px;
    margin: 0 auto;
    color: #000000;
    font-weight: bold;
}

.titlebar .title {
    float: left;
    width: 50%;
    _width: 48%;
}

.titlebar .toobar {
    float: right;
    width: 40%;
    font-size: 12px;
    text-align: right;
    padding-right: 10px;
    cursor: pointer;
}

/*--- formated table ---*/
.formtable {
    height: 21px;
    line-height: 21px;
    border: none;
	margin:10px auto;
}

.formtable td {
    text-align: left;
    border:none;
}

.formtable th {
    text-align: right;
    background-color: #fff;
    border: none;
	border-right: 1px solid #a9c3d0;
}

.formtable th,.formtable td {
    vertical-align: middle;
    padding: 3px;
    font-weight: normal;
}

.formtable td input[type="text"],.formtable td input[type="password"],.formtable td select
{
    height: 22px;
    line-height: 22px;
	border:none;
    border-bottom: 1px solid #D7D7D7;
    /* 	width: 96%; */
}

.formtable td textarea {
    border:none;
    border-bottom: 1px solid #D7D7D7;
    width: 96%;
}

.formtable .subtitle {
    color: #b30d0d;
}

.formtable td input.lose , .formtable td select.lose {border:none;border-bottom:1px solid #D70000;color:red;}
.formtable td input[type="radio"] {border-color:#FFFFFF;}
.formtable td input[type="radio"].lose{border-color:#D70000;}

.listtradd{
    background: rgb(255, 255, 255);
}
.listtreven{
    /* background: #F7F7F7; */
    background: rgb(255, 255, 255);
}
.listtrhover{
    background: #EEE;
}
.listtrselect{
    background: #e6e6e6;
    color:#0A7FAE;
}

.pagination {
    background: url(../../images/page_bg.gif) repeat-x;
    padding: 0px;
}

.pagination p.left {
    float: left;
    margin-left: 15px;
    height: 22px;
    line-height: 24px;
}

.pagination p.right {
    float: right;
    margin-right: 5px;
    height: 22px;
    line-height: 24px;
}

* html .pagination p.right {
    float: right;
    height: 20px;
    line-height: 24px;
    margin-top: 6px;
    margin-right: 5px;
}

* html .pagination font {
    font-size: 13px;
    vertical-align: top;
}

*+html .pagination font {
    vertical-align: middle;
    font-size: 12px;
}

.pagination p.right img {
    margin: 3px 2px 4px 2px;
}

* html .pagination p.right img {
    margin: -1px 2px 4px 2px;
}

*+html .pagination p.right img {
    margin: 4px 2px 4px 2px;
}

.stepbar {
    background: url(../../images/stepbg.gif) repeat-x;
    border-right: 1px solid #cfcfcf;
    border-left: 1px solid #cfcfcf;
    height: 26px;
    margin-bottom: 5px;
}

.stepbar ul li {
    float: left;
    padding: 0px 20px;
    height: 26px;
    line-height: 26px;
    list-style: none;
    background: url(../../images/step02.gif) no-repeat bottom right;
}

.stepbar ul li.stepleft {
    width: 10px;
    height: 26px;
    background: url(../../images/step03.gif) no-repeat;
    padding: 0;
    margin-left: -10px;
}

.stepbar ul li.steplight {
    color: #FFF;
    background: url(../../images/steplightbg.gif) no-repeat right top;
}
.borderDiv{
    border-bottom: 1px solid #A9C3D0;
    border-left: 1px solid #A9C3D0;
    border-right:  1px solid #A9C3D0;
}

/*----report-----*/
.postcontent {
    padding: 10px 10px 70px 10px;
}
.postcontent i {left: 50%;text-align: center; background: none; font-size: 16px; font-weight: bold; height: 55px;}
.postcontent span {font-size: 14px; font-weight: normal;}

.posttable th , .posttable td {height:26px;line-height:26px;padding:0 5px;table-layout:fixed;}
.posttable {table-layout:fixed;}
.posttable th {vertical-align: top;}
.posttable td {vertical-align: bottom;}
.posttable td span.test{color:#F00;}
.posttable td label {
    height: 26px;
    line-height: 26px;
    padding-right: 3px;
    vertical-align: middle;
    width:12px;
}
.posttable td label input[type="radio"] {
    height: 12px;
    line-height: 26px;
    vertical-align: middle;
    padding-right: 3px;
    padding-bottom: 2px;
}
.posttable td label.text {
    height: 26px;
    line-height: 26px;
    vertical-align: middle;
}
/*background-color:#E8F2FF*/
.postcontent th {border-right:1px solid #A9C3D0;text-align:right;font-weight:normal;}
.postcontent td input[type="text"] {border:none;border-bottom:1px solid #e6e6e6;width:75%;background-color: #f1f8f8;}
.postcontent td textarea {border:none;border-bottom:1px solid #e6e6e6;width:75%;}
.postcontent td select{border:none;border-bottom:1px solid #e6e6e6; background-color: #f1f8f8;width: 75%;}
.postcontent td input[type="text"].lose {border:none;border-bottom:2px solid #ff0000;width:75%;color:#000;}
.postcontent td input[type="password"].lose:focus{border-color:#FF5722!important}
.postcontent td select.lose{border:none;border-bottom:2px solid #ff0000;color:#000;}
.postcontent td span.test{color:#F00;}
.postcontent td label.lose {color:#f00;}
.postcontent td textarea.lose {border:none;border-bottom:2px solid #ff0000;width:75%;color:#000;}
.postcontent td  button.lose {border-bottom:2px solid #ff0000;}
.postdiv fieldset {text-align: left; padding: 0; margin-bottom:10px;padding:5px;}
.postdiv fieldset legend {padding:0 5px;color:#404040;}

.topfield {margin-top:-12px;}

/*-----startpostcontentprint-----*/
.postcontentprint {
    padding: 0 10px 30px 10px;
}
.postcontentprint i {left: 50%;text-align: center; background: none; font-size: 24px; font-weight: bold; height: 45px;}
.postcontentprint span {font-size: 12px; font-weight: normal;}
.postcontentprint th {border-right:1px solid #A9C3D0;text-align:right;font-weight:normal;}
.postcontentprint td input[type="text"] {border:none;border-bottom:1px solid #000;width:75%;background-color: #ffffff;font-size: 12px;}
.postcontentprint td textarea {border:none;border-bottom:1px solid #000;width:75%;}
.postcontentprint td select{border:none;border-bottom:1px solid #000; background-color: #f1f8f8;}
.postcontentprint td input[type="text"].lose {border:none;border-bottom:2px solid #ff0000;width:75%;color:#000;font-size: 12px;}
.postcontentprint td select.lose{border:none;border-bottom:2px solid #ff0000;color:#000;}
.postcontentprint td span.test{color:#F00;}
.postcontentprint td label.lose {color:#f00;}
.postcontentprint td textarea.lose {border:none;border-bottom:2px solid #ff0000;width:75%;color:#000;}
.postcontentprint td  button.lose {border-bottom:2px solid #ff0000;}

.printposttable th , .printposttable td {height:21px;line-height:21px;padding:0 5px;table-layout:fixed;}
.printposttable {table-layout:fixed;}
.printposttable th {vertical-align: top;}
.printposttable td {vertical-align: bottom;}
.printposttable td span.test{color:#F00;}
.printposttable td label {
    height: 21px;
    line-height: 21px;
    padding-right: 3px;
    vertical-align: middle;
    width:12px;
}
.printposttable td label input[type="radio"] {
    height: 12px;
    line-height: 21px;
    vertical-align: middle;
    padding-right: 3px;
    padding-bottom: 2px;
}
.printposttable td label.text {
    height: 21px;
    line-height: 21px;
    vertical-align: middle;
}

/*-----endpostcontentprint-----*/


/*----*/
.postcontentehr td input[type="text"] {
    width:70%;
}
.postcontentehr td span {
    margin-left: 10px;
}

