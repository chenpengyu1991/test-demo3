package com.founder.rhip.portal.common;

/**
 * 互动管理状态
 * @version 1.0, 2013-2-26
 */
public enum InteractionStatus {
	
	/*已分派*/
	ASSIGN("01"),
	
	/*已接受*/
	ACCEPT("02"),
	
	/*已退回*/
	RETURN("03"),
	
	/*已回复*/
	REPLY("04"),
	
	/*新增*/
	NEW("05");
	
   private String value;
   
   InteractionStatus(String value) {
        this.value = value;
    }

   public String getValue() {
       return this.value;
   }
}
