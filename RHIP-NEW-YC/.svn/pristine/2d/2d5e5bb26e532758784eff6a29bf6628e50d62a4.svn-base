package com.founder.rhip.hsa.common;

/**
 * 报告登记状态
 * 
 * @author liuk
 * 
 */
public enum RecordStatus {
	/** 待确认 */
	READY("1"),
	/** 已确认 */
	CONFIRMED("2"),
	/** 已处理 */
	DEALED("3"),
	/** 待回访 */
	TO_VISIT("4"),
	/** 已经回访 */
	VISITED("5");

	private String value;

	RecordStatus(String value) {
		this.value = value;
	}

	public String getValue() {
		return this.value;
	}
}
