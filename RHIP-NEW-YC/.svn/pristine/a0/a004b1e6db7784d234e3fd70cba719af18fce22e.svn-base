package com.founder.rhip.oh.controller.form;

import java.io.Serializable;
import java.util.Date;
/**
 * 农药中毒查询表单参数
 * @author duankun
 *
 */
public class PesticidePoisonReportForm implements Serializable {

	private static final long serialVersionUID = 1L;

	private String name;
	
	private String idCard;
	
	private Date startDate;
	
	private Date endDate;
	
	private String reportCardType;
	
	private String poisonPesticideType;
	
	private String poisonType;
	
	private String poisonDegree;
	
	private String outCome;
	
	private String checkCondition;

	private String drugType;
	
	public String getDrugType() {
		return drugType;
	}

	public void setDrugType(String drugType) {
		this.drugType = drugType;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIdCard() {
		return idCard;
	}

	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getReportCardType() {
		return reportCardType;
	}

	public void setReportCardType(String reportCardType) {
		this.reportCardType = reportCardType;
	}

	public String getPoisonPesticideType() {
		return poisonPesticideType;
	}

	public void setPoisonPesticideType(String poisonPesticideType) {
		this.poisonPesticideType = poisonPesticideType;
	}

	public String getPoisonType() {
		return poisonType;
	}

	public void setPoisonType(String poisonType) {
		this.poisonType = poisonType;
	}

	public String getPoisonDegree() {
		return poisonDegree;
	}

	public void setPoisonDegree(String poisonDegree) {
		this.poisonDegree = poisonDegree;
	}

	public String getOutCome() {
		return outCome;
	}

	public void setOutCome(String outCome) {
		this.outCome = outCome;
	}

	public String getCheckCondition() {
		return checkCondition;
	}

	public void setCheckCondition(String checkCondition) {
		this.checkCondition = checkCondition;
	}

}
