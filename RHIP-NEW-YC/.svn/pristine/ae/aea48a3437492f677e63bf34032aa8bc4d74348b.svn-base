<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
<xs:include schemaLocation="BASE.xsd"/>
  <xs:element name="RD_BED_DISTRIBUTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ORGAN_CODE"/>
        <xs:element ref="ORGAN_NAME"/>
        <xs:element ref="GENRE_CODE"/>
        <xs:element ref="YEAR_MONTH"/>
        <xs:element ref="YEAR"/>
        <xs:element ref="HALF_YEAR"/>
        <xs:element ref="QUARTER"/>
        <xs:element ref="MONTH"/>
        <xs:element ref="REPORT_TYPE"/>
        <xs:element ref="AUTHORIZED_BED"/>
        <xs:element ref="REAL_BED"/>
        <xs:element ref="ACTUAL_AVAILABLE_BED_DAYS"/>
        <xs:element ref="ACTUAL_USING_BED_DAYS"/>
        <xs:element ref="INPATIENT_BED_DAYS"/>
        <xs:element ref="OBSERVATION_BED"/>
        <xs:element ref="FAMILY_BEDS" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AUTHORIZED_BED" type="xs:integer"/>
  <xs:element name="REAL_BED" type="xs:integer"/>
  <xs:element name="ACTUAL_AVAILABLE_BED_DAYS" type="xs:integer"/>
  <xs:element name="ACTUAL_USING_BED_DAYS" type="xs:integer"/>
  <xs:element name="INPATIENT_BED_DAYS" type="xs:integer"/>
  <xs:element name="OBSERVATION_BED" type="xs:integer"/>
  <xs:element name="FAMILY_BEDS" type="integer-or-empty"/>
</xs:schema>
